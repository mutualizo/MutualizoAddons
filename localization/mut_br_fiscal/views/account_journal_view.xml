<?xml version="1.0" ?>
<odoo>
    <record id="mut_view_account_journal_form_inherited" model="ir.ui.view">
        <field name="name">mut.account.journal.form.inherited</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form" />
        <field name="arch" type="xml">
            <field name="currency_id" position="after">
                <field name="statement_sequence_id" invisible="type not in ['bank','cash']"/>
            </field>
        </field>
    </record>
</odoo>
